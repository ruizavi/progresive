{
  "name": "progresive",
  "version": "1.0.0",
  "author": "Avi (Victor Ruiz)",
  "description": "Abstraction Module For Express/Typescript",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruizavi/progresive.git"
  },
  "main": "./dist/progresive/index.cjs",
  "module": "./dist/progresive/index.js",
  "typings": "./dist/progresive/index.d.cts",
  "exports": {
    ".": {
      "import": "./dist/progresive/index.js",
      "require": "./dist/progresive/index.cjs",
      "types": "./dist/progresive/index.d.cts"
    }
  },
  "files": [
    "dist",
    "README"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsup",
    "start:dev": "tsnd --respawn src/main.ts",
    "lint": "npx @biomejs/biome lint src",
    "format": "npx @biomejs/biome format src",
    "lint:format": "npx @biomejs/biome check --apply src",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@biomejs/biome": "1.3.3",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.99",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.9.5",
    "@types/passport": "^1.0.16",
    "esbuild": "0.19.8",
    "husky": "^8.0.0",
    "lint-staged": "^15.1.0",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "passport": "^0.6.0",
    "reflect-metadata": "^0.1.13",
    "ts-pattern": "^5.0.5"
  },
  "keywords": [
    "express",
    "decorators",
    "typescript",
    "backend"
  ],
  "license": "ISC",
  "lint-staged": {
    "*.ts?(x)": [
      "npx @biomejs/biome check --apply src"
    ]
  }
}
